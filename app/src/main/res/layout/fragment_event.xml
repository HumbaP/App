    android:layout_height="match_parent"
    tools:context="mx.com.sisei.www.sisei.fragments.EventFragment">

    <Button
        android:id="@+id/button_conferences"
        android:text="Conferencias"
        android:layout_gravity="center"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content" />

    <Button
        android:id="@+id/button_workshops"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="center"
        android:text="Talleres"
        android:layout_marginTop="@dimen/bottom_nav"/>
</FrameLayout>
